<h1><a href="DisplayCD">Chefs</a> | Dishes</h1>
@model List<Dish2>

<h3>Yum, take a look at our tasty Dishes</h3>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Chef</th>
            <th scope="col">Tastiness</th>
            <th scope="col"> Calories</th>
        </tr>
    </thead>
    <tbody>
        
        
        @foreach (var item in ViewBag.AllDishes)
        {
        <tr>
            <td>@item.Name</td>
            <td>@item.Cook.ChefFirstName </td>
            <td>@item.Tastiness</td>
            <td>@item.Calories</td>

        </tr>
        }
        

    </tbody>
</table>
<a href="AddDish">Add A Dish</a>